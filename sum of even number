package com.company;

public class cwp_25_practice_set_4 {
    public static void main(String[] args) {
        int sum = 0;
        int n= 3;
        for(int i =0;i<n;i++){
            sum = sum + (2*i);
        }
        System.out.print("sum of even number is:");
        System.out.println(sum);
    }
}
